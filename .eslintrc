{
  "extends": ["next", "next/core-web-vitals"],
  "plugins": ["jest", "@typescript-eslint", "prettier"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "prettier/prettier": "error",
    "@typescript-eslint/no-unused-vars": "error",
    "no-console": "error",
    "jest/expect-expect": [
      "error",
      {
        "assertFunctionNames": ["expect"]
      }
    ]
  }
}
