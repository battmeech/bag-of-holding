{
  "name": "nextchakra-starter",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ./src --ext ts",
    "prettify": "prettier --write src",
    "schema:download": "apollo client:download-schema graphql-schema.json",
    "schema:codegen": "apollo client:codegen --target=typescript --passthroughCustomScalars --customScalarsPrefix=GraphQL$ --tsFileExtension=ts",
    "schema:download:dev": "apollo client:download-schema graphql-schema.json -v=dev",
    "schema:codegen:dev": "apollo client:codegen --target=typescript --passthroughCustomScalars --customScalarsPrefix=GraphQL$ --tsFileExtension=ts -v=dev"
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@chakra-ui/icons": "^1.0.14",
    "@chakra-ui/react": "^1.6.5",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@fontsource/lexend": "^4.5.0",
    "framer-motion": "^4.1.17",
    "graphql": "^15.5.1",
    "next": "^11.0.0",
    "next-pwa": "^5.2.23",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@netlify/plugin-nextjs": "^3.6.3",
    "@types/react": "^17.0.14",
    "apollo": "^2.33.4",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  }
}
